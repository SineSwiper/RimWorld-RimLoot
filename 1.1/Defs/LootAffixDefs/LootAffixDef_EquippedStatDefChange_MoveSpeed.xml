<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <RimLoot.LootAffixDef>
        <defName>Jogging</defName>
        <label>Jogging</label>
        <groupName>EquippedStatDefChange_MoveSpeed</groupName>
        <modifiers>
            <li Class="RimLoot.LootAffixModifier_EquippedStatDefChange">
                <affectedStat>MoveSpeed</affectedStat>
                <addValue>0.15</addValue>
            </li>
        </modifiers>
        <affixCost>2</affixCost>
        <affixRulePack>
            <rulesStrings>
                <li>possessive->Jogger's</li>
                <li>adjective->Jogging</li>
                <li>noun->Jogger</li>
                <li>verb->Jogging</li>
                <li>noun_is_singular->True</li>
            </rulesStrings>
        </affixRulePack>
    </RimLoot.LootAffixDef>

