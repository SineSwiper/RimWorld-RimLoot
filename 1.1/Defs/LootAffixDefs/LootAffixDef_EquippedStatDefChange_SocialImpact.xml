        <label>Acting</label>
        <groupName>EquippedStatDefChange_SocialImpact</groupName>
        <modifiers>
            <li Class="RimLoot.LootAffixModifier_EquippedStatDefChange">
                <affectedStat>SocialImpact</affectedStat>
                <multiplier>1.5</multiplier>
            </li>
            <li Class="RimLoot.LootAffixModifier_EquippedStatDefChange">
                <affectedStat>PawnBeauty</affectedStat>
                <addValue>0.5</addValue>
            </li>
        </modifiers>
        <affixCost>1.5</affixCost>
        <affixRulePack>
            <rulesStrings>
                <li>possessive->Actor's</li>
