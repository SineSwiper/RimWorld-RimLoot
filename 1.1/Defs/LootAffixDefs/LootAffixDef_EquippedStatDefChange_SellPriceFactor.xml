    <RimLoot.LootAffixDef>
        <defName>Duchessy</defName>
        <label>Duchessy</label>
        <groupName>EquippedStatDefChange_SellPriceFactor</groupName>
        <modifiers>
            <li Class="RimLoot.LootAffixModifier_EquippedStatDefChange">
                <affectedStat>SellPriceFactor</affectedStat>
                <multiplier>1.5</multiplier>
            </li>
            <li Class="RimLoot.LootAffixModifier_EquippedStatDefChange">
                <affectedStat>RoyalFavorValue</affectedStat>
                <multiplier>1.5</multiplier>
            </li>
        </modifiers>
        <affixCost>2</affixCost>
        <affixRulePack>
            <rulesStrings>
                <li>possessive->Duke's</li>
                <li>adjective->Duchessy</li>
                <li>noun->Duke</li>
                <li>noun_is_singular->True</li>
            </rulesStrings>
        </affixRulePack>
    </RimLoot.LootAffixDef>

